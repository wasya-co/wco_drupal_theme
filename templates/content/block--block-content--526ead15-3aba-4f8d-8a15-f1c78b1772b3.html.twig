{#
  social links, social menu
#}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}

<ul class="menu social-links">
  <li class="menu-item linkedin">
    <a href="https://www.linkedin.com/company/wasya-co">
      <span>LinkedIn</span>
      <img width="32" height="32" style="width: 32px; height: 32px;" alt='linkedin logo' src="data:image/png;base64," />
    </a>
  </li>
  <li class="menu-item youtube">
    <a href="https://www.youtube.com/@wasyaco">
      <span>Youtube</span>
      <img width="32" height="32" style="width: 32px; height: 32px;" alt='youtube logo' src="data:image/png;base64," />
    </a>
  </li>
</ul>

  {% endblock %}
</div>
